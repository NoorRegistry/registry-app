{
  "cli": {
    "appVersionSource": "remote",
    "version": ">= 14.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development",
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "preview",
        "NODE_ENV": "preview"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "production",
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production",
        "releaseStatus": "draft"
      },
      "ios": {
        "appleId": "noorregistryapp@gmail.com",
        "ascAppId": "6749792410"
      }
    },
    "preview": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "appleId": "noorregistryapp@gmail.com",
        "ascAppId": "6749792410"
      }
    }
  }
}
